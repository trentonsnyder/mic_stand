<p>EVENTS DUDE</p>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Expiration</th>
      <th>Duration</th>
      <th>Time Left</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |e| %>
      <tr>
        <td><%= link_to e.name, event_path(e) %></td>
        <td><%= e.phone_number.phone_formatted %></td>
        <td><%= e.formatted_expiry %></td>
        <td><%= Time.zone.at(e.duration).utc.strftime("%H:%M:%S") %></td>
        <td><%= e.time_left %></td>
      </tr>
    <% end%>
  </tbody>
</table>